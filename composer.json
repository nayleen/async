{
    "name": "nayleen/async-runtimes",
    "description": "",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "amphp/amp": "^3",
        "amphp/log": "^2",
        "nayleen/finder": "~0",
        "php-di/php-di": "^6",
        "revolt/event-loop": "^1"
    },
    "require-dev": {
        "amphp/http-server": "^3",
        "amphp/redis": "^2",
        "nayleen/development": "^2@dev",
        "roave/better-reflection": "^6",
        "roave/security-advisories": "dev-latest"
    },
    "conflict": {
        "vimeo/psalm": "*"
    },
    "suggest": {
        "amphp/http-server": "To run http server workers",
        "amphp/redis": "To use Redis as the backing queue for busses"
    },
    "autoload": {
        "psr-4": {
            "Nayleen\\Async\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayleen\\Async\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
